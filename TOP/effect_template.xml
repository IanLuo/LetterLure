<effect>
	<fillPattern alpha="1">
       <patternArray>
			<pattern type="1" width="50" height="50" alpha="1" isStencil="1"><!--0 for generic, 1 for image pattern -->
                <imagePatternPath>pattern3.png</imagePatternPath>
                <systemPatternKey></systemPatternKey>
            </pattern>
        </patternArray>
	   <gradient type="0" blendMode="0"> <!-- 1 for radial, 0 for line -->
		   <gradientValue red="0" green="0" blue="0" alpha="0" location="0"/>
		   <gradientValue red="255" green="255" blue="255" alpha="0.5" location="1"/>
		   <lineGradientAttrs angle="0"/>
		   <radialGradientAttrs center1XPercentage="" center1YPercentage="" center2XPercentage="" center2YPercentage="" radialGradientRadius1Percentage="" radialGradientRadius2Percentage=""/>
	   </gradient>
    </fillPattern>
	
     <strokePattern alpha="0.7">
        <patternArray>
			<pattern type="0" width="50" height="50" alpha="0.5">
                <imagePatternPath></imagePatternPath>
                <systemPatternKey>patternGrid</systemPatternKey>
            </pattern>
        </patternArray>
		
		<strokeGradient type="1" blendMode="0"> <!-- 1 for radial, 0 for line -->
			<strokeGradientValue red="255" green="255" blue="255" alpha="0.5" location="0"/>
			<strokeGradientValue red="20" green="30" blue="40" alpha="0.5" location="0.8"/>
			<strokeGradientValue red="0" green="0" blue="0" alpha="0.5" location="1"/>
			<lineStrokeGradientAttrs angle=""/>
			<radialStrokeGradientAttrs center1XPercentage="0.5" center1YPercentage="0.5" center2XPercentage="0.5" center2YPercentage="0.5" radialGradientRadius1Percentage="1" radialGradientRadius2Percentage="0"/>
		</strokeGradient>
    </strokePattern>

    <strokeWidthPercent value="0.03"/>
	<strokeColor red="10" green="200" blue="100" alpha="1"/><!-- if stroke gradient is defined above, this will not work -->
    <shadowColor red="10" green="20" blue="30" alpha="0.5"/>
    <shadowOffset widthPercentage="0.2" heightPercentage="0.2"/>
    <shadowBlur value="0"/>
</effect>