<effect>
	<fillPattern alpha="1">
		<patternArray>
			<pattern type="1" width="25" height="25" alpha="0.5" isStencil="1"><!--0 for generic, 1 for image pattern -->
				<imagePatternPath>circles1.png</imagePatternPath>
			</pattern>
		</patternArray>
		
		<gradient type="0" blendMode="1"> <!-- 1 for radial, 0 for line -->
			<gradientValue red="21" green="15" blue="10" alpha="1.0" location="0.3"/>
			<gradientValue red="89" green="37" blue="13" alpha="1.0" location="0.36"/>
			<gradientValue red="191" green="100" blue="53" alpha="1.0" location="0.45"/>
			<gradientValue red="107" green="109" blue="97" alpha="1.0" location="0.5"/>
			<gradientValue red="46" green="62" blue="78" alpha="1.0" location="0.7"/>
			<lineGradientAttrs angle="-90"/>
			<radialGradientAttrs center1XPercentage="" center1YPercentage="" center2XPercentage="" center2YPercentage="" radialGradientRadius1Percentage="" radialGradientRadius2Percentage=""/>
		</gradient>

    </fillPattern>
	
	<strokePattern alpha="1.0">
		<strokeGradient type="0" blendMode="1"> <!-- 1 for radial, 0 for line -->
			<strokeGradientValue red="21" green="15" blue="10" alpha="1.0" location="0.2"/>
			<strokeGradientValue red="89" green="37" blue="13" alpha="1.0" location="0.26"/>
			<strokeGradientValue red="191" green="100" blue="53" alpha="1.0" location="0.35"/>
			<strokeGradientValue red="107" green="109" blue="97" alpha="1.0" location="0.5"/>
			<strokeGradientValue red="46" green="62" blue="78" alpha="1.0" location="0.7"/>
			<lineStrokeGradientAttrs angle="90"/>
		</strokeGradient>
	</strokePattern>
	
	<strokeWidthPercent value="0.01"/>
	<shadowColor red="0" green="0" blue="0" alpha="0.0"/>
	<shadowOffset widthPercentage="-0.0" heightPercentage="-0.0"/>
	<shadowBlur value="0"/>
</effect>