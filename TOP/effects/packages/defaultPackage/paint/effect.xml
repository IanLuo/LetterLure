		   <gradientValue red="153" green="157" blue="109" alpha="1.0" location="0.3"/>
		   <gradientValue red="191" green="191" blue="150" alpha="1.0" location="0.5"/>
		   <gradientValue red="204" green="202" blue="179" alpha="1.0" location="0.7"/>
		   <gradientValue red="202" green="206" blue="179" alpha="1.0" location="0.8"/>
		   <gradientValue red="169" green="180" blue="180" alpha="1.0" location="0.9"/>
		   <gradientValue red="169" green="180" blue="180" alpha="1.0" location="1"/>
		   <lineGradientAttrs angle="90"/>
		   <radialGradientAttrs center1XPercentage="" center1YPercentage="" center2XPercentage="" center2YPercentage="" radialGradientRadius1Percentage="" radialGradientRadius2Percentage=""/>
	   </gradient>
    </fillPattern>
	
    <strokeWidthPercent value="0.01"/>
