<effect>
	<fillPattern alpha="1">
       <patternArray>
			<pattern type="1" width="150" height="150" alpha="0.5" isStencil="1"><!--0 for generic, 1 for image pattern -->
                <imagePatternPath>grain3.png</imagePatternPath>
                <systemPatternKey></systemPatternKey>
            </pattern>
        </patternArray>
	   <gradient type="0" blendMode="1"> <!-- 1 for radial, 0 for line -->
		   <gradientValue red="108" green="123" blue="73" alpha="1.0" location="0"/>
