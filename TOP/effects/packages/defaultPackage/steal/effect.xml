<effect>
	<fillPattern alpha="1">
		<gradient type="0" blendMode="1"> <!-- 1 for radial, 0 for line -->
			<gradientValue red="155" green="122" blue="98" alpha="1.0" location="0.0"/>
			<gradientValue red="255" green="255" blue="255" alpha="1.0" location="0.4"/>
			<gradientValue red="255" green="255" blue="255" alpha="1.0" location="0.6"/>
			<gradientValue red="155" green="122" blue="98" alpha="1.0" location="1.0"/>
			<lineGradientAttrs angle="90.0"/>
			<radialGradientAttrs center1XPercentage="1.0" center1YPercentage="1.0" center2XPercentage="1.0" center2YPercentage="1.0" radialGradientRadius1Percentage="1.0" radialGradientRadius2Percentage="0.0"/>
		</gradient>
    </fillPattern>
	
	<strokePattern alpha="0.7">
