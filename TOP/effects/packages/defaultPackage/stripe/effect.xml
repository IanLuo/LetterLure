<effect>
	<fillPattern alpha="1">
		<gradient type="0" blendMode="9"> <!-- 1 for radial, 0 for line -->
			<gradientValue red="230" green="141" blue="42" alpha="1" location="0.2"/>
			<gradientValue red="230" green="141" blue="42" alpha="1" location="0.4"/>
			<gradientValue red="218" green="94" blue="41" alpha="1" location="0.4"/>
			<gradientValue red="218" green="94" blue="41" alpha="1" location="0.6"/>
			<gradientValue red="230" green="141" blue="42" alpha="1" location="0.6"/>
			<gradientValue red="230" green="141" blue="42" alpha="1" location="0.8"/>
			<gradientValue red="218" green="94" blue="41" alpha="1" location="0.8"/>

			<lineGradientAttrs angle="90.0"/>
			<radialGradientAttrs center1XPercentage="1.0" center1YPercentage="1.0" center2XPercentage="1.0" center2YPercentage="1.0" radialGradientRadius1Percentage="1.0" radialGradientRadius2Percentage="0.0"/>
		</gradient>
    </fillPattern>
	
	<strokePattern alpha="1.0">
		<strokeGradient type="0" blendMode="0"> <!-- 1 for radial, 0 for line -->
			<strokeGradientValue red="230" green="141" blue="42" alpha="1" location="0.2"/>
			<strokeGradientValue red="230" green="141" blue="42" alpha="1" location="0.4"/>
			<strokeGradientValue red="218" green="94" blue="41" alpha="1" location="0.4"/>
			<strokeGradientValue red="218" green="94" blue="41" alpha="1" location="0.6"/>
			<strokeGradientValue red="230" green="141" blue="42" alpha="1" location="0.6"/>
			<strokeGradientValue red="230" green="141" blue="42" alpha="1" location="0.8"/>
			<strokeGradientValue red="218" green="94" blue="41" alpha="1" location="0.8"/>
			<lineStrokeGradientAttrs angle="-90"/>
			<radialStrokeGradientAttrs center1XPercentage="0.5" center1YPercentage="0.5" center2XPercentage="0.5" center2YPercentage="0.5" radialGradientRadius1Percentage="1" radialGradientRadius2Percentage="0"/>
		</strokeGradient>
    </strokePattern>

	<strokeWidthPercent value="0.01"/>
	<shadowColor red="196" green="106" blue="101" alpha="0.0"/>
	<shadowOffset widthPercentage="-0.0" heightPercentage="-0.0"/>
	<shadowBlur value="0"/>
</effect>