<effect>
	<fillPattern alpha="1">
       <patternArray>
			<pattern type="1" width="150" height="150" alpha="0.5" isStencil="1"><!--0 for generic, 1 for image pattern -->
                <imagePatternPath>kneadedPaper.png</imagePatternPath>
                <systemPatternKey></systemPatternKey>
            </pattern>
        </patternArray>
	   <gradient type="0" blendMode="9"> <!-- 1 for radial, 0 for line -->
		   <gradientValue red="0" green="34" blue="117" alpha="1.0" location="0"/>
		   <gradientValue red="39" green="218" blue="232" alpha="1.0" location="0.4"/>
		   <gradientValue red="96" green="216" blue="228" alpha="1.0" location="0.5"/>
		   <gradientValue red="0" green="117" blue="195" alpha="1.0" location="0.6"/>
		   <gradientValue red="0" green="34" blue="117" alpha="1.0" location="1.0"/>
		   <lineGradientAttrs angle="90"/>
		   <radialGradientAttrs center1XPercentage="" center1YPercentage="" center2XPercentage="" center2YPercentage="" radialGradientRadius1Percentage="" radialGradientRadius2Percentage=""/>
	   </gradient>
    </fillPattern>
	
     <strokePattern alpha="0.7">
        <!--<patternArray>
			<pattern type="0" width="50" height="50" alpha="0.5">
                <imagePatternPath></imagePatternPath>
                <systemPatternKey>patternGrid</systemPatternKey>
            </pattern>
        </patternArray>-->
		
		<strokeGradient type="1" blendMode="0"> <!-- 1 for radial, 0 for line -->
			<strokeGradientValue red="32" green="134" blue="202" alpha="1.0" location="0.0"/>
			<strokeGradientValue red="39" green="218" blue="232" alpha="1.0" location="0.4"/>
			<strokeGradientValue red="96" green="216" blue="228" alpha="1.0" location="0.5"/>
			<strokeGradientValue red="39" green="218" blue="232" alpha="1.0" location="0.4"/>
			<strokeGradientValue red="32" green="134" blue="202" alpha="1.0" location="1.0"/>
			<lineStrokeGradientAttrs angle=""/>
			<radialStrokeGradientAttrs center1XPercentage="0.5" center1YPercentage="0.9" center2XPercentage="0.5" center2YPercentage="0.9" radialGradientRadius1Percentage="1" radialGradientRadius2Percentage="0"/>
		</strokeGradient>
    </strokePattern>

    <strokeWidthPercent value="0.02"/>
	<strokeColor red="10" green="200" blue="100" alpha="1"/><!-- if stroke gradient is defined above, this will not work -->
    <shadowColor red="10" green="20" blue="30" alpha="0.0"/>
    <shadowOffset widthPercentage="0.0" heightPercentage="0.0"/>
    <shadowBlur value="0"/>
</effect>