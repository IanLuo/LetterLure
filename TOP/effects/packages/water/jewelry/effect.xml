<effect>
	<fillPattern alpha="1">
       <patternArray>
			<pattern type="1" width="150" height="150" alpha="1" isStencil="1"><!--0 for generic, 1 for image pattern -->
                <imagePatternPath>kneadedPaper.png</imagePatternPath>
                <systemPatternKey></systemPatternKey>
            </pattern>
        </patternArray>
	   <gradient type="0" blendMode="9"> <!-- 1 for radial, 0 for line -->
		   <gradientValue red="252" green="236" blue="252" alpha="1.0" location="0"/>
		   <gradientValue red="251" green="166" blue="225" alpha="1.0" location="0.5"/>
		   <gradientValue red="253" green="137" blue="215" alpha="1.0" location="0.51"/>
		   <gradientValue red="255" green="124" blue="216" alpha="1.0" location="1.0"/>
		   <lineGradientAttrs angle="90"/>
		   <radialGradientAttrs center1XPercentage="" center1YPercentage="" center2XPercentage="" center2YPercentage="" radialGradientRadius1Percentage="" radialGradientRadius2Percentage=""/>
	   </gradient>
    </fillPattern>
	
     <strokePattern alpha="0.7">
        <!--<patternArray>
			<pattern type="0" width="50" height="50" alpha="0.5">
                <imagePatternPath></imagePatternPath>
                <systemPatternKey>patternGrid</systemPatternKey>
            </pattern>
        </patternArray>-->
		
		<strokeGradient type="0" blendMode="0"> <!-- 1 for radial, 0 for line -->
			<strokeGradientValue red="240" green="183" blue="161" alpha="1.0" location="0"/>
			<strokeGradientValue red="140" green="51" blue="16" alpha="1.0" location="0.5"/>
			<strokeGradientValue red="117" green="34" blue="1" alpha="1.0" location="0.51"/>
			<strokeGradientValue red="191" green="110" blue="78" alpha="1.0" location="1.0"/>
			<lineStrokeGradientAttrs angle=""/>
			<radialStrokeGradientAttrs center1XPercentage="0.5" center1YPercentage="0.5" center2XPercentage="0.5" center2YPercentage="0.5" radialGradientRadius1Percentage="1" radialGradientRadius2Percentage="0"/>
		</strokeGradient>
    </strokePattern>

    <strokeWidthPercent value="0.02"/>
	<strokeColor red="10" green="200" blue="100" alpha="1"/><!-- if stroke gradient is defined above, this will not work -->
    <shadowColor red="10" green="20" blue="30" alpha="0.0"/>
    <shadowOffset widthPercentage="0.0" heightPercentage="0.0"/>
    <shadowBlur value="0"/>
</effect>