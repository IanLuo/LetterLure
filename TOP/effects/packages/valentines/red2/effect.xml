<effect>
    <fillPattern alpha="1">
        <gradient type="1" blendMode="1"> <!-- 1 for radial, 0 for line -->
			<gradientValue red="19" green="2" blue="2" alpha="1.0" location="0"/>
			<gradientValue red="198" green="70" blue="78" alpha="1.0" location="0.5"/>
			<gradientValue red="231" green="112" blue="135" alpha="1.0" location="1.0"/>

		   <radialGradientAttrs center1XPercentage="0.5" center1YPercentage="0.5" center2XPercentage="0.5" center2YPercentage="0.5" radialGradientRadius1Percentage="1.0" radialGradientRadius2Percentage="0.0"/>
	   </gradient>
    </fillPattern>

    <strokePattern alpha="1">
		<strokeGradient type="0">
			<strokeGradientValue red="19" green="2" blue="2" alpha="1" location="0"/>
			<strokeGradientValue red="198" green="70" blue="78" alpha="1" location="0.5"/>
			<strokeGradientValue red="231" green="112" blue="135" alpha="1" location="1"/>

			<lineStrokeGradientAttrs angle="45"/>
			<radialStrokeGradientAttrs center1XPercentage="0.5" center1YPercentage="0.5" center2XPercentage="0.5" center2YPercentage="0.5" radialGradientRadius1Percentage="1" radialGradientRadius2Percentage="0"/>
		</strokeGradient>
    </strokePattern>
		
    <strokeWidthPercent value="0.04"/>
	<!--<strokeColor red="255" green="255" blue="255" alpha="1"/>-->
    <shadowColor red="0" green="0" blue="0" alpha="0.5"/>
    <shadowOffset widthPercentage="0.0" heightPercentage="0.0"/>
    <shadowBlur value="0"/>
</effect>