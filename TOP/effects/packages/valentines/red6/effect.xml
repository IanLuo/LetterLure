<effect>
    <fillPattern alpha="1">
		<gradient type="0" blendMode="0"> <!-- 1 for radial, 0 for line -->
			<gradientValue red="235" green="233" blue="234" alpha="1.0" location="0"/>
			<gradientValue red="255" green="255" blue="255" alpha="1.0" location="1"/>
			<lineGradientAttrs angle="0"/>
			<radialGradientAttrs center1XPercentage="0.5" center1YPercentage="0.5" center2XPercentage="0.5" center2YPercentage="0.5" radialGradientRadius1Percentage="0.0" radialGradientRadius2Percentage="1.0"/>
		</gradient>
    </fillPattern>

	<strokePattern alpha="1.0">
		<strokeGradient type="0" blendMode="2"> <!-- 1 for radial, 0 for line -->
			<strokeGradientValue red="218" green="58" blue="34" alpha="1.0" location="0"/>
			<strokeGradientValue red="190" green="52" blue="31" alpha="1.0" location="0.5"/>
			<strokeGradientValue red="218" green="58" blue="34" alpha="1.0" location="1"/>
			<lineStrokeGradientAttrs angle="45"/>
			<radialStrokeGradientAttrs center1XPercentage="0.5" center1YPercentage="0.5" center2XPercentage="0.5" center2YPercentage="0.5" radialGradientRadius1Percentage="1" radialGradientRadius2Percentage="0"/>
		</strokeGradient>
	</strokePattern>

    <strokeWidthPercent value="0.1"/>
	<strokeColor red="0" green="0" blue="0" alpha="1"/>
    <shadowColor red="255" green="255" blue="255" alpha="0.5"/>
    <shadowOffset widthPercentage="0.02" heightPercentage="0.02"/>
    <shadowBlur value="3"/>
</effect>