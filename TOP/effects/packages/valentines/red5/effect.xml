<effect>
    <fillPattern alpha="1">
		<patternArray>
			<pattern type="1" width="63" height="38" alpha="1" isStencil="1"><!--0 for generic, 1 for image pattern -->
                <imagePatternPath>dirty.png</imagePatternPath>
                <systemPatternKey></systemPatternKey>
            </pattern>
        </patternArray>
    </fillPattern>

	<strokePattern alpha="1.0">
		<strokeGradient type="1" blendMode="2"> <!-- 1 for radial, 0 for line -->
			<strokeGradientValue red="228" green="199" blue="132" alpha="1.0" location="0"/>
			<strokeGradientValue red="150" green="122" blue="96" alpha="1.0" location="0.5"/>
			<strokeGradientValue red="228" green="199" blue="132" alpha="1.0" location="1"/>
			<lineStrokeGradientAttrs angle=""/>
			<radialStrokeGradientAttrs center1XPercentage="0.5" center1YPercentage="0.5" center2XPercentage="0.5" center2YPercentage="0.5" radialGradientRadius1Percentage="1" radialGradientRadius2Percentage="0"/>
		</strokeGradient>
	</strokePattern>

    <strokeWidthPercent value="0.1"/>
	<strokeColor red="0" green="0" blue="0" alpha="1"/>
    <shadowColor red="0" green="0" blue="0" alpha="0.5"/>
    <shadowOffset widthPercentage="0.02" heightPercentage="0.02"/>
    <shadowBlur value="2"/>
</effect>