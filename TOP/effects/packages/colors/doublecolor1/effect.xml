<effect>
	<fillPattern alpha="1">
	   <gradient type="0" blendMode="1"> <!-- 1 for radial, 0 for line -->
		   <gradientValue red="69" green="59" blue="9" alpha="1.0" location="0.0"/>
		   <gradientValue red="69" green="59" blue="9" alpha="1.0" location="0.5"/>
		   <gradientValue red="246" green="228" blue="75" alpha="1.0" location="0.5"/>
		   <gradientValue red="246" green="228" blue="75" alpha="1.0" location="1.0"/>
		   <lineGradientAttrs angle="45"/>
	   </gradient>
    </fillPattern>

	<strokeWidthPercent value="0.1"/>
	<strokeColor red="0" green="0" blue="0" alpha="1"/><!-- if stroke gradient is defined above, this will not work -->
	<shadowColor red="255" green="255" blue="255" alpha="1.0"/>
	<shadowOffset widthPercentage="0.0" heightPercentage="0.0"/>
	<shadowBlur value="0"/>
</effect>