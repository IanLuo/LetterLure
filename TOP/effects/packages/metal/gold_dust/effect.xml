<effect>
    <fillPattern alpha="1">
        <patternArray>
			<pattern type="1" width="50" height="50" alpha="1">
                <imagePatternPath>squared_metal.png</imagePatternPath>
                <systemPatternKey></systemPatternKey>
            </pattern>
        </patternArray>
        <gradient type="0"> <!-- 1 for radial, 0 for line -->
		   <gradientValue red="167" green="112" blue="11" alpha="1" location="0"/>
            <gradientValue red="255" green="247" blue="69" alpha="1" location="1"/>
			<gradientValue red="167" green="112" blue="11" alpha="1" location="0"/>
            <gradientValue red="255" green="247" blue="69" alpha="1" location="1"/>
